{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 800,
  "height": 450,
  "title": "Malaysia State Boundaries with Rates",
  "projection": {
    "type": "equalEarth",
    "scale": 2500,
    "center": [109, 4]
  },
  "layer": [
    {
      "data": {
        "url": "https://nesdai.github.io/FTT3179_Homework_w9/data/geoBoundaries-MYS-ADM1_simplified.json",
        "format": { "type": "json" }
      },
      "mark": { "type": "geoshape", "stroke": "white" },
      "encoding": {
        "tooltip": [
          { "field": "properties.shapeName", "type": "nominal" },
          { "field": "properties.Rate", "type": "quantitative" }
        ]
      }
    }
  ]
}
